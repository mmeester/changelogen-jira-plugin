{
  "name": "changelogen-jira-plugin",
  "version": "0.0.0",
  "description": "JIRA integration plugin for changelogen",
  "repository": "https://github.com/mmeester/changelogen-jira-plugin",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "author": {
    "name": "Maurits Meester",
    "email": "maurits@e-mmer.nl"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "changelogen",
    "plugin",
    "jira",
    "changelog",
    "conventional-commits"
  ],
  "scripts": {
    "--- ▶️ Application Tools ---": "",
    "build": "unbuild",
    "dev": "vitest dev",
    "prepack": "pnpm build",
    "--- 🎨 Linting | ✅ Checks ---": "",
    "lint": "eslint . && prettier -c src test",
    "--- 🎨 Linting | 🔧 Fix ---": "",
    "lint:fix": "eslint --fix . && prettier -w src test",
    "--- 🧪 Tests ---": "",
    "test": "pnpm lint && vitest run --coverage",
    "test:watch": "vitest",
    "--- 🚀 Release ": "",
    "release": "changelogen --release --push"
  },
  "peerDependencies": {
    "changelogen": ">=0.6.0",
    "consola": "^3.4.2"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@vitest/coverage-v8": "^3.2.4",
    "changelogen": "^0.6.2",
    "consola": "^3.4.2",
    "eslint": "^9.30.1",
    "eslint-config-unjs": "^0.5.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.12.4"
}
